<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="felipe.example.com.pacientes">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icone"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.Intro">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.MenuPaciente"
            android:label="@string/lista_paciente" >
        </activity>
        <activity
            android:name=".Activity.NovoPaciente"
            android:label="@string/cadastro_paciente" >
        </activity>
        <activity
            android:name=".Activity.ListaPaciente"
            android:label="@string/lista_paciente" >
        </activity>
        <activity
            android:name=".Activity.VisualizarPaciente"
            android:label="@string/lista_paciente" >
        </activity>
    </application>

</manifest>